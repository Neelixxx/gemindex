generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model AppState {
  id        String   @id
  version   Int
  data      Json
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model IngestionRun {
  id          String   @id @default(cuid())
  source      String
  status      String
  startedAt   DateTime @default(now())
  finishedAt  DateTime?
  summary     String?
  error       String?
  recordCount Int?
}
